<!-- Google Map -->
<div id="map-area">
    <%= render "map"%>
</div>
<!-- End Google Map -->

<!-- Contact Section -->
<div id="contact" class="page">
<div class="container">
    <!-- Title Page -->
    <div class="row">
        <div class="span12">
            <div class="title-page">
                <h2 class="title">预约</h2>
               
            </div>
        </div>
    </div>
    <!-- End Title Page -->
    
    <!-- Contact Form -->
    <div class="row">
      <div class="span9">
          <%= form_for @yuyue do |f| %>
            <%= f.label :name ,"姓名" %>
            <%= f.text_field :name %>
            <%= f.label :email, "E-mail" %>
            <%= f.text_field :email %>
            <%= f.label :qq, "QQ" %>
            <%= f.text_field :qq %>
            <%= f.label :phone, "联系电话" %>
            <%= f.text_field :phone %>
            <p>产品分类</p>
            <%= f.select :type_id, Type.all.map { |c| [c.name, c.id] }%>
            <%= f.label :bei, "备注" %>
            <%= f.text_area :bei, class: "span9"%>
            <%= f.submit "提交", class: "btn btn-warning"%>
          <% end %>
        </div>
        
        <div class="span3">
          <div class="contact-details">
            <h3>联系我们</h3>
                <ul>
                    <li>邮箱：378480605@qq.com</li>
                    <li>联系电话：13810978592</li>
                    <li>
                        联系人：宁晓鹏
                        </li>
                        <li>
                        地址：北京市海淀区文慧园路6号三缔时代科技发展有限责任公司
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <!-- End Services Form -->
</div>
</div>
<!-- End Contact Section -->


