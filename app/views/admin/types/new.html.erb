<%= render "layouts/top"%>
<div class="container-fluid" style="padding-top: 1%">
  <div class="row-fluid">
    <div class="span2">
      <%= render "layouts/left" %>
    </div>
    <div class="span10">
      <%= form_for [:admin, @type] do |f| %>
        <%= f.label :name, "添加分类名称"%>
        <%= f.text_field :name %>
        <p>
          <%= f.submit "添加", class: "btn btn-primary"%>
        </p>
      <% end %>
    </div>
  </div>
</div>



